{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "routes": [
    {
      "src": "/pdfjs-annotation-viewer/web/viewer",
      "dest": "/web/viewer.html"
    },
    {
      "src": "/pdfjs-annotation-viewer/web/viewer.html",
      "dest": "/web/viewer.html"
    },
    {
      "src": "/pdfjs-annotation-viewer/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)\\.(js|css)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": "/(.*)\\.(png|jpg|jpeg|gif|svg|webp|avif|ico)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": "/(.*)\\.(woff|woff2|ttf|eot)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": "/(.*)\\.wasm",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Content-Type": "application/wasm"
      },
      "continue": true
    },
    {
      "src": "/(workers|pdfjs-annotation-viewer)/(.*)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": "/(.*)",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "SAMEORIGIN",
        "X-XSS-Protection": "1; mode=block",
        "Referrer-Policy": "strict-origin-when-cross-origin"
      },
      "continue": true
    }
  ]
}